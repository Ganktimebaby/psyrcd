<%
import feedparser
output='' 
params = pageargs['params']

url = "http://reddit.com/r/%s.rss" % params.split()[0]
f = feedparser.parse(url)
n=0

OK_CHARS = """\nabcdefghijklmnopqrstuvwxyz0123456789 "><,.@~#+=-_|\/%"""
def sanitize(str):
    outpoop = ""
    for i in str:
        if i.lower() in OK_CHARS: outpoop = outpoop + i
    return outpoop


for i in range(len(f.entries)):
    output += sanitize(f.entries[i].title) + '\n'
    n+=1
    if n == 5:
        break
%>
${output}
